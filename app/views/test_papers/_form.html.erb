<div class="pb-test-wrapper pb-container">
  <h2 class="test-title">自我挑戰</h2>

  <div class="row">
    <div class="col-md-2">
      <%= simple_form_for(@test_paper) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :customized_concept_id %>
        <%= f.input :user %>
      </div>

      <div class="form-actions">
        <%= f.button :submit %>
      </div>
    <% end %>
    </div>

    <div class="col-md-10">
      <iframe src=<%= @question%> width="900" height="780" style="border: none;"> </iframe>
    </div>
    
    
  </div>
  

</div>

<div class="row">
  <% @courses.each do |course| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <div class="caption">
          <h3><%= link_to(course.title, course_path(course)) %></h3>
          <p><%= course.overview %></p>
          <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
        </div>
      </div>
    </div>
  <% end %>
</div>

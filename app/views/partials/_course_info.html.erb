  <thead>
    <tr>
      <th>已建立課程</th>
      <th>Overview</th>
    </tr>
  </thead>
  <tbody>
    <% my_courses.each do |c| %>
    <tr>
      <td><%= c.title %></td>
      <td><%= c.overview %></td>
    </tr>
    <% end %>
  </tbody>
</table>




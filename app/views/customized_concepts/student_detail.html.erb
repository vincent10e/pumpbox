<div class="pb-index-wrapper pb-container">
  <div class="row">

    <div class="col-md-9 col-md-push-3">
      

      <table class="table table-striped table-hover">
        <thead>
          <h1> <%= @concept.title%> </h1>
          <tr>
            <th>題目</th>
            <th>錯誤次數 </th>
            <th>時間</th>
          </tr>
        </thead>

        <tbody>
          <% @tests.each do |test| %>
          <tr>
            <td><%= test.question%></td>
            <td></td>
            <td><%= @test_attempts.last.updated_at %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
     
      
    </div>
    <div class="col-md-3 col-md-pull-9">
      <div class="list-group">
        
      </div>
    </div>
  </div>
</div>